{
  "name": "little_league_bot",
  "version": "1.0.0",
  "main": "server.js",
  "author": "Simon VDB",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.16.1",
    "bufferutil": "^3.0.1",
    "discord.js": "https://github.com/hydrabolt/discord.js.git",
    "discord.js-commando": "https://github.com/Gawdl3y/discord.js-commando.git",
    "dotenv": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^7.0.22",
    "tslint": "^5.3.2",
    "typescript": "^2.3.3"
  },
  "scripts": {
    "transpile": "tsc --watch",
    "start": "node build/server.js",
    "deploy": "tsc && now --public && now alias"
  },
  "now": {
    "alias": "little-league",
    "files": [
      "build/**/*.js"
    ],
    "env": {
      "NODE_ENV": "production",
      "BOT_SECRET_PROD": "@lll_bot_secret_prod",
      "BOT_SECRET_DEV": "@lll_bot_secret_dev",
      "RIOT_TOKEN": "@ll_riot_token"
    }
  }
}